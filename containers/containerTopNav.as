package containers.topNav{	import flash.display.Sprite;	import classes.utils.ListHolder;		/**	 * @author juliancertain	 */	public class ButtonContainer extends Sprite {				// Members		private var _config : XMLList;		private var _formats : XMLList;						public function ButtonContainer() {						_config = ListHolder.getInstance('config').list;			_formats = ListHolder.getInstance('formats').list;						this.graphics.beginFill(0xFF0000);			this.graphics.drawRect(0, 0, _formats.buttonContainer_width, _formats.buttonContainer_height);			this.graphics.endFill();		}	}}