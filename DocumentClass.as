package{	//importieren der benötigten externen Klassen	import flash.display.MovieClip;	import flash.utils.Timer;	import flash.events.TimerEvent;		//Vererbung	public class DocumentClass extends MovieClip	{		private var timer:Timer = new Timer(100);				//konstruktor		public function DocumentClass():void		{			timer.addEventListener(TimerEvent.TIMER, makeStar);			timer.start();		}		//funktion		private function makeStar(e:TimerEvent):void		{			//Stern instanzieren			var star:Star = new Star();			star.scaleX = star.scaleY = Math.random();			star.x = Math.random() * this.stageWidth			star.y = Math.random() * this.stageHeight			this.addChild(star);		}			}}